{
  "name": "@testquality/sdk",
  "version": "1.10.0",
  "description": "SDK for accessing TestQuality API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && tsc --project ./tsconfig.json",
    "dev": "tsc --watch",
    "clean": "rimraf dist/*",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format:fix": "prettier --write '(src|test)/**/*.ts'",
    "prepublish": "yarn build",
    "bundle:build": "npx webpack",
    "bundle:serve": "npx lite-server --baseDir=build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitModern/test-quality-sdk.git"
  },
  "author": "james@bitmodern.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitModern/test-quality-sdk/issues"
  },
  "homepage": "https://github.com/BitModern/test-quality-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^27.2.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^27.2.4",
    "lite-server": "^2.6.1",
    "prettier": "^2.3.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.2.1",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "testquality",
    "test",
    "quality",
    "sdk"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "querystring": "^0.2.1"
  }
}
